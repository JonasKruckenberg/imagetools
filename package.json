    "cross-env": "7.0.3",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "rollup": "2.75.5",
    "tslib": "2.4.0",
    "typedoc": "0.22.17",
    "typedoc-plugin-markdown": "3.12.1",
    "typescript": "4.7.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
